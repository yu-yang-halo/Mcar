<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <ImageView
        android:layout_width="260dp"
        android:layout_height="80dp"
        android:id="@+id/imageView5"
       android:background="@mipmap/car0"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="14dp"
        android:layout_height="126dp"
        android:id="@+id/btne1"
        android:background="@drawable/skbottomcolor"
        android:layout_marginLeft="104dp"
        android:layout_marginStart="102dp"
        android:layout_marginBottom="31dp"
        android:layout_alignBottom="@+id/imageView5"
        android:layout_alignLeft="@+id/imageView5"
        android:layout_alignStart="@+id/imageView5" />

    <Button
        android:layout_width="14dp"
        android:layout_height="126dp"
        android:id="@+id/btnf1"
        android:background="@drawable/skbottomcolor"
        android:layout_marginLeft="44dp"
        android:layout_marginStart="44dp"
        android:layout_alignTop="@+id/btne1"
        android:layout_toRightOf="@+id/btne1"
        android:layout_toEndOf="@+id/btne1" />

    <Button
        android:layout_width="14dp"
        android:layout_height="126dp"
        android:id="@+id/btnc1"
        android:background="@drawable/sktopcolor"
        android:layout_below="@+id/btnf1"
        android:layout_alignLeft="@+id/imageView5"
        android:layout_alignStart="@+id/imageView5"
        android:layout_marginLeft="22dp"
        android:layout_marginStart="22dp" />

    <Button
        android:layout_width="14dp"
        android:layout_height="86dp"
        android:id="@+id/btnj1"
        android:background="@drawable/sktopcolor"
        android:layout_marginRight="14dp"
        android:layout_marginEnd="14dp"
        android:layout_below="@+id/btnf1"
        android:layout_alignRight="@+id/btnf1"
        android:layout_alignEnd="@+id/btnf1" />

    <Button
        android:layout_width="14dp"
        android:layout_height="126dp"
        android:id="@+id/btnd1"
        android:background="@drawable/sktopcolor"
        android:layout_below="@+id/btnf1"
        android:layout_alignRight="@+id/imageView5"
        android:layout_alignEnd="@+id/imageView5"
        android:layout_marginRight="12dp"
        android:layout_marginEnd="12dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="左前车门"
        android:id="@+id/textView23"
        android:layout_alignTop="@+id/btne1"
        android:layout_alignLeft="@+id/btnc1"
        android:layout_alignStart="@+id/btnc1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="左后车门"
        android:id="@+id/textView24"
        android:layout_alignTop="@+id/btnf1"
        android:layout_toRightOf="@+id/btnf1"
        android:layout_toEndOf="@+id/btnf1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="左前翼子板"
        android:id="@+id/textView25"
        android:layout_alignBottom="@+id/btnc1"
        android:layout_toRightOf="@+id/btnc1"
        android:layout_toEndOf="@+id/btnc1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="左裙边"
        android:id="@+id/textView26"
        android:layout_alignBottom="@+id/btnj1"
        android:layout_toLeftOf="@+id/btnj1"
        android:layout_toStartOf="@+id/btnj1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="左后翼子板"
        android:id="@+id/textView27"
        android:layout_below="@+id/btnd1"
        android:layout_alignLeft="@+id/textView24"
        android:layout_alignStart="@+id/textView24" />

</RelativeLayout>
