<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <ImageView
        android:layout_width="260dp"
        android:layout_height="80dp"
        android:id="@+id/imageView5"
       android:background="@mipmap/car0"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="14dp"
        android:layout_height="126dp"
        android:id="@+id/button6"
        android:background="@mipmap/skbottom0"
        android:layout_marginLeft="104dp"
        android:layout_marginStart="102dp"
        android:layout_marginBottom="31dp"
        android:layout_alignBottom="@+id/imageView5"
        android:layout_alignLeft="@+id/imageView5"
        android:layout_alignStart="@+id/imageView5" />

    <Button
        android:layout_width="14dp"
        android:layout_height="126dp"
        android:id="@+id/button7"
        android:background="@mipmap/skbottom0"
        android:layout_marginLeft="44dp"
        android:layout_marginStart="44dp"
        android:layout_alignTop="@+id/button6"
        android:layout_toRightOf="@+id/button6"
        android:layout_toEndOf="@+id/button6" />

    <Button
        android:layout_width="14dp"
        android:layout_height="126dp"
        android:id="@+id/button8"
        android:background="@mipmap/sktop0"
        android:layout_below="@+id/button7"
        android:layout_alignLeft="@+id/imageView5"
        android:layout_alignStart="@+id/imageView5"
        android:layout_marginLeft="22dp"
        android:layout_marginStart="22dp" />

    <Button
        android:layout_width="14dp"
        android:layout_height="86dp"
        android:id="@+id/button9"
        android:background="@mipmap/sktop0"
        android:layout_marginRight="14dp"
        android:layout_marginEnd="14dp"
        android:layout_below="@+id/button7"
        android:layout_alignRight="@+id/button7"
        android:layout_alignEnd="@+id/button7" />

    <Button
        android:layout_width="14dp"
        android:layout_height="126dp"
        android:id="@+id/button10"
        android:background="@mipmap/sktop0"
        android:layout_below="@+id/button7"
        android:layout_alignRight="@+id/imageView5"
        android:layout_alignEnd="@+id/imageView5"
        android:layout_marginRight="12dp"
        android:layout_marginEnd="12dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="E1-左前车门"
        android:id="@+id/textView23"
        android:layout_alignTop="@+id/button6"
        android:layout_alignLeft="@+id/imageView5"
        android:layout_alignStart="@+id/imageView5" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="F1-左后车门"
        android:id="@+id/textView24"
        android:layout_alignTop="@+id/button7"
        android:layout_toRightOf="@+id/button7"
        android:layout_toEndOf="@+id/button7" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="C1-左前翼子板"
        android:id="@+id/textView25"
        android:layout_alignBottom="@+id/button8"
        android:layout_toRightOf="@+id/button8"
        android:layout_toEndOf="@+id/button8" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="J1-左裙边"
        android:id="@+id/textView26"
        android:layout_alignBottom="@+id/button9"
        android:layout_toLeftOf="@+id/button9"
        android:layout_toStartOf="@+id/button9" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="D1-左后翼子板"
        android:id="@+id/textView27"
        android:layout_below="@+id/button10"
        android:layout_alignLeft="@+id/textView24"
        android:layout_alignStart="@+id/textView24" />

</RelativeLayout>
